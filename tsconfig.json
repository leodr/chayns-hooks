{
	"compilerOptions": {
		"declaration": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": ["ESNext", "DOM"],
		"outDir": "dist",
		"strict": true,
		"target": "ES5"
	},
	"include": ["src"],
	"typedocOptions": {
		"categoryOrder": ["Functions", "*"],
		"excludeNotExported": true,
		"hideBreadcrumbs": true,
		"hideGenerator": true,
		"hideSources": true,
		"inputFiles": ["./src"],
		"mode": "file",
		"name": null,
		"out": "temp-docs",
		"plugin": "typedoc-plugin-markdown",
		"readme": "./readme_template.md"
	}
}
